{
    "files.exclude": {
        ".vscode": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/venv": true,
        "*.code-workspace": true,
        ".pylintrc": true,
        ".gitignore": true
    }
    ,"python.linting.pycodestyleEnabled": true
    ,"python.linting.pycodestyleArgs": ["--max-line-length=160"]
    ,"python.linting.pylintEnabled": true
    ,"python.linting.pylintArgs": ["--disable", "line-too-long, no-name-in-module, import-error"]
}